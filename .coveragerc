[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if non-runnable code isn't run:
    if __name__ == .__main__.:

omit = *\__init__.py
        backend\src\test*
        backend\src\main\game\room\concrete_room_cards*
        backend\src\main\game\monster\concrete_monster_cards*